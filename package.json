{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "create-iydheko-stack",
  "version": "0.1.0-alpha.2",
  "description": "A quick starter template by Ferdinan Iydheko",
  "bin": {
    "create-iydheko-stack": "./dist/bin/index.js"
  },
  "type": "module",
  "main": "dist/bin/index.js",
  "private": false,
  "files": ["dist", "README.md", "LICENSE"],
  "scripts": {
    "build": "tsc",
    "dev": "tsx bin/index.ts",
    "start": "node dist/bin/index.js",
    "prepublishOnly": "bun run build && node ./scripts/ensure-shebang.cjs"
  },
  "keywords": ["react", "tanstack", "cli", "starter", "iydheko", "Ferdinan Iydheko"],
  "author": "Ferdinan Iydheko",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18",
    "bun": ">=1.1.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^12.0.2",
    "@types/bun": "latest",
    "@types/chalk": "^2.2.4",
    "@types/commander": "^2.12.5",
    "@types/execa": "^2.0.2",
    "@types/node": "^24.6.2",
    "@types/prompts": "^2.4.9",
    "semantic-release": "^24.2.9",
    "tsx": "^4.20.6"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "execa": "^9.6.0",
    "prompts": "^2.4.2"
  }
}
